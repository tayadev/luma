-- Control flow variations
let classify = fn(n: Number): String do
    if n < 0 do
        return "negative"
    else do
        if n == 0 do
            return "zero"
        else do
            return "positive"
        end
    end
end

-- Test classification
let r1 = classify(-5)
let r2 = classify(0)
let r3 = classify(10)

-- Loop with break
var count = 0
var i = 0
while i < 100 do
    if i > 5 do
        break
    end
    count = count + 1
    i = i + 1
end

count
